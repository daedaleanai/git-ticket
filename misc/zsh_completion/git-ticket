#compdef _git-ticket git-ticket


function _git-ticket {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "add:Create a new ticket."
      "assign:Assign a user to a ticket."
      "ccb:Change Control Board (CCB) actions of a ticket."
      "commands:Display available commands."
      "comment:Display or add comments to a ticket."
      "config:List configs or show the specified config"
      "deselect:Clear the implicitly selected ticket."
      "label:Display, add or remove labels to/from a ticket."
      "ls:List tickets."
      "ls-id:List ticket identifiers."
      "ls-label:List valid labels."
      "pull:Pull tickets update from a git remote."
      "push:Push tickets update to a git remote."
      "review:Review actions of a ticket."
      "rm:Remove an existing bug."
      "select:Select a ticket for implicit use in future commands."
      "show:Display the details of a ticket."
      "status:Display or change a ticket status."
      "termui:Launch the terminal UI."
      "title:Display or change a title of a ticket."
      "user:Display or change the user identity."
      "validate:Validate identities and commits signatures."
      "version:Show git-ticket version information."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  add)
    _git-ticket_add
    ;;
  assign)
    _git-ticket_assign
    ;;
  ccb)
    _git-ticket_ccb
    ;;
  commands)
    _git-ticket_commands
    ;;
  comment)
    _git-ticket_comment
    ;;
  config)
    _git-ticket_config
    ;;
  deselect)
    _git-ticket_deselect
    ;;
  label)
    _git-ticket_label
    ;;
  ls)
    _git-ticket_ls
    ;;
  ls-id)
    _git-ticket_ls-id
    ;;
  ls-label)
    _git-ticket_ls-label
    ;;
  pull)
    _git-ticket_pull
    ;;
  push)
    _git-ticket_push
    ;;
  review)
    _git-ticket_review
    ;;
  rm)
    _git-ticket_rm
    ;;
  select)
    _git-ticket_select
    ;;
  show)
    _git-ticket_show
    ;;
  status)
    _git-ticket_status
    ;;
  termui)
    _git-ticket_termui
    ;;
  title)
    _git-ticket_title
    ;;
  user)
    _git-ticket_user
    ;;
  validate)
    _git-ticket_validate
    ;;
  version)
    _git-ticket_version
    ;;
  esac
}

function _git-ticket_add {
  _arguments \
    '(-t --title)'{-t,--title}'[Provide a title to describe the issue]:' \
    '(-m --message)'{-m,--message}'[Provide a message to describe the issue]:' \
    '(-F --file)'{-F,--file}'[Take the message from the given file. Use - to read the message from the standard input]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_assign {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_ccb {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "add:Add a CCB member as an approver of a ticket status."
      "approve:Approve a ticket status."
      "block:Block a ticket status."
      "rm:Remove a CCB member as an approver of a ticket status."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  add)
    _git-ticket_ccb_add
    ;;
  approve)
    _git-ticket_ccb_approve
    ;;
  block)
    _git-ticket_ccb_block
    ;;
  rm)
    _git-ticket_ccb_rm
    ;;
  esac
}

function _git-ticket_ccb_add {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_ccb_approve {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_ccb_block {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_ccb_rm {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_commands {
  _arguments \
    '(-p --pretty)'{-p,--pretty}'[Output the command description as well as Markdown compatible comment]' \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_comment {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "add:Add a new comment to a ticket."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  add)
    _git-ticket_comment_add
    ;;
  esac
}

function _git-ticket_comment_add {
  _arguments \
    '(-F --file)'{-F,--file}'[Take the message from the given file. Use - to read the message from the standard input]:' \
    '(-m --message)'{-m,--message}'[Provide the new message from the command line]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_config {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "set:Set the named configuration data."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  set)
    _git-ticket_config_set
    ;;
  esac
}

function _git-ticket_config_set {
  _arguments \
    '(-F --file)'{-F,--file}'[Take the config from the given file. Use - to read the config from the standard input]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_deselect {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_label {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "add:Add a label to a ticket."
      "rm:Remove a label from a ticket."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  add)
    _git-ticket_label_add
    ;;
  rm)
    _git-ticket_label_rm
    ;;
  esac
}

function _git-ticket_label_add {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_label_rm {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_ls {
  _arguments \
    '(*-s *--status)'{\*-s,\*--status}'[Filter by status. Valid values are [proposed,vetted,inprogress,inreview,reviewed,accepted,merged,done,rejected,ALL]]:' \
    '(*-a *--author)'{\*-a,\*--author}'[Filter by author]:' \
    '(*-p *--participant)'{\*-p,\*--participant}'[Filter by participant]:' \
    '*--actor[Filter by actor]:' \
    '(*-A *--assignee)'{\*-A,\*--assignee}'[Filter by assignee]:' \
    '(*-l *--label)'{\*-l,\*--label}'[Filter by label]:' \
    '(*-t *--title)'{\*-t,\*--title}'[Filter by title]:' \
    '(*-n *--no)'{\*-n,\*--no}'[Filter by absence of something. Valid values are [label]]:' \
    '(-b --by)'{-b,--by}'[Sort the results by a characteristic. Valid values are [id,creation,edit]]:' \
    '(-d --direction)'{-d,--direction}'[Select the sorting direction. Valid values are [asc,desc]]:' \
    '(-f --format)'{-f,--format}'[Select the output formatting style. Valid values are [default,plain,json,org-mode]]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_ls-id {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_ls-label {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_pull {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_push {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_review {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "checklist:Complete a checklist associated with a ticket."
      "clear:Clear the Differential Revision data associated with a ticket."
      "fetch:Get Differential Revision data from Phabricator and store in a ticket."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  checklist)
    _git-ticket_review_checklist
    ;;
  clear)
    _git-ticket_review_clear
    ;;
  fetch)
    _git-ticket_review_fetch
    ;;
  esac
}

function _git-ticket_review_checklist {
  _arguments \
    '(-b --blank)'{-b,--blank}'[Discard any previously edited checklist and start again with a blank one]' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_review_clear {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_review_fetch {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_rm {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_select {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_show {
  _arguments \
    '(-t --timeline)'{-t,--timeline}'[Output the timeline of the ticket]' \
    '--field[Select field to display. Valid values are [assignee,author,authorEmail,ccb,checklists,createTime,lastEdit,humanId,id,labels,reviews,shortId,status,nextStatuses,title,workflow,actors,participants]]:' \
    '(-f --format)'{-f,--format}'[Select the output formatting style. Valid values are [default,json,org-mode]]:' \
    '(-s --since)'{-s,--since}'[Limit the timeline to changes since the given date/time. Valid formats are: yyyy-mm-ddThh:mm:ss OR yyyy-mm-dd]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_status {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "accepted:Ticket is set ACCEPTED."
      "done:Ticket is set DONE."
      "inprogress:Ticket is set IN PROGRESS."
      "inreview:Ticket is set IN REVIEW."
      "merged:Ticket is set MERGED."
      "proposed:Ticket is set PROPOSED."
      "rejected:Ticket is set REJECTED."
      "reviewed:Ticket is set REVIEWED."
      "vetted:Ticket is set VETTED."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  accepted)
    _git-ticket_status_accepted
    ;;
  done)
    _git-ticket_status_done
    ;;
  inprogress)
    _git-ticket_status_inprogress
    ;;
  inreview)
    _git-ticket_status_inreview
    ;;
  merged)
    _git-ticket_status_merged
    ;;
  proposed)
    _git-ticket_status_proposed
    ;;
  rejected)
    _git-ticket_status_rejected
    ;;
  reviewed)
    _git-ticket_status_reviewed
    ;;
  vetted)
    _git-ticket_status_vetted
    ;;
  esac
}

