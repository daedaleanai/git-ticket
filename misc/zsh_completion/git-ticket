function _git-ticket_title {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "edit:Edit a title of a ticket."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  edit)
    _git-ticket_title_edit
    ;;
  esac
}

function _git-ticket_title_edit {
  _arguments \
    '(-t --title)'{-t,--title}'[Provide a title to describe the issue]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_user {
  local -a commands

  _arguments -C \
    '(-f --field)'{-f,--field}'[Select field to display. Valid values are [email,humanId,id,keys,lastModification,lastModificationLamport,login,metadata,name,phabId]]:' \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "adopt:Adopt an existing identity as your own."
      "create:Create a new identity."
      "edit:Edit a user identity."
      "key:Display, add or remove keys to/from a user."
      "ls:List identities."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  adopt)
    _git-ticket_user_adopt
    ;;
  create)
    _git-ticket_user_create
    ;;
  edit)
    _git-ticket_user_edit
    ;;
  key)
    _git-ticket_user_key
    ;;
  ls)
    _git-ticket_user_ls
    ;;
  esac
}

function _git-ticket_user_adopt {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_user_create {
  _arguments \
    '--key-file[Take the armored PGP public key from the given file. Use - to read the message from the standard input]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_user_edit {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}


function _git-ticket_user_key {
  local -a commands

  _arguments -C \
    '--rebuild-cache[force the cache to be rebuilt]' \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "add:Add a PGP key from a user."
      "rm:Remove a PGP key from the adopted or the specified user."
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  add)
    _git-ticket_user_key_add
    ;;
  rm)
    _git-ticket_user_key_rm
    ;;
  esac
}

function _git-ticket_user_key_add {
  _arguments \
    '(-F --file)'{-F,--file}'[Take the armored PGP public key from the given file. Use - to read the message from the standard input]:' \
    '(-k --key)'{-k,--key}'[Provide the armored PGP public key from the command line]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_user_key_rm {
  _arguments \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_user_ls {
  _arguments \
    '(-f --format)'{-f,--format}'[Select the output formatting style. Valid values are [default,json]]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_validate {
  _arguments \
    '(-e --external)'{-e,--external}'[Validate commit in an external repository at this path]:' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

function _git-ticket_version {
  _arguments \
    '(-n --number)'{-n,--number}'[Only show the version number]' \
    '(-c --commit)'{-c,--commit}'[Only show the commit hash]' \
    '(-a --all)'{-a,--all}'[Show all version information]' \
    '--rebuild-cache[force the cache to be rebuilt]'
}

